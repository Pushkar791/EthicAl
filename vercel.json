{
  "version": 2,
  "builds": [
    {
      "src": "simple.py",
      "use": "@vercel/python"
    },
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.py",
      "use": "@vercel/python"
    },
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "debug.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/debug",
      "dest": "debug.py"
    },
    {
      "src": "/simple",
      "dest": "simple.py"
    },
    {
      "src": "/",
      "dest": "simple.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "simple.py"
    }
  ]
} 