{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.py",
      "use": "@vercel/python"
    },
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/",
      "dest": "index.py"
    },
    {
      "src": "/(.*)",
      "dest": "wsgi.py"
    }
  ]
} 